{
  "baseUrl": "https://qa.polars.io/",
  "userAgent": "synpress",
  "retries": { "runMode": 0, "openMode": 0 },
  "defaultCommandTimeout": 100000,
  "pageLoadTimeout": 80000,
  "requestTimeout": 80000,
  "integrationFolder": "cypress/integration",
  "screenshotsFolder": "/screenshots",
